{
  "name": "react-cra-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "element-react": "^1.4.34",
    "element-theme-default": "^1.4.13",
    "history": "^5.3.0",
    "hygen": "^6.2.11",
    "postcss": "^8.4.16",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.2",
    "react": "^18.2.0",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.0",
    "react-scripts": "5.0.1",
    "resolve-url-loader": "^4.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "serve": "npm run serve:dev",
    "serve:dev": "dotenv -e .env.serve-dev react-app-rewired start",
    "serve:release": "dotenv -e .env.serve-release react-app-rewired start",
    "serve:prod": "dotenv -e .env.serve-prod react-app-rewired start",
    "serve:mock": "dotenv -e .env.serve-mock react-app-rewired start",
    "build": "npm run build:dev",
    "build:dev": "dotenv -e .env.build-dev react-app-rewired build",
    "build:release": "dotenv -e .env.build-release react-app-rewired build",
    "build:prod": "dotenv -e .env.build-prod react-app-rewired build",
    "build:analyzer": "cross-env use_analyzer=true npm run build",
    "lint:all": "npm run lint:prettier & npm run lint:stylelint",
    "lint:prettier": "prettier --write --list-different --loglevel warn \"**/*.{js,json,css,less,html}\"",
    "lint:stylelint": "stylelint src/**/*.{css,less} --fix",
    "test": "react-app-rewired jest",
    "eject": "react-app-rewired eject",
    "new:component": "hygen component new"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "commitizen": "^4.2.5",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "customize-cra": "^1.0.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "prettier": "^2.7.1",
    "react-app-rewired": "^2.2.1",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^28.0.0",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
